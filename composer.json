{
    "name": "mvqn/ucrm-module-rest",
    "description": "Some core code for use when developing UCRM Plugins.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-json": "*",
        "ext-curl": "*",
        "mvqn/common": "1.0.0",
        "mvqn/annotations": "1.0.5",
        "mvqn/collections": "1.0.0",
        "mvqn/rest": "1.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "7.2.7",
        "vlucas/phpdotenv": "2.5.1"
    },
    "autoload": {
        "psr-4": {
            "UCRM\\": "src/UCRM/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/UCRM/"
        }
    }
}